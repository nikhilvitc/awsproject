{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "your-account-id.dkr.ecr.us-east-1.amazonaws.com/awsproject-backend:latest",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "5000"
    }
  ],
  "Environment": [
    {
      "Name": "NODE_ENV",
      "Value": "production"
    },
    {
      "Name": "AWS_REGION",
      "Value": "us-east-1"
    }
  ],
  "Logging": "/var/log/eb-docker/containers/eb-current-app"
}
